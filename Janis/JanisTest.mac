/run/initialize
/tracking/verbose 1

/random/setSeeds 1321 4456

# Set the distance between the generator and the target
/generator/setDistance 40

/process/list

# Farside detector at (0 deg, 140 cm) by default
/run/printProgress 10
/run/beamOn 200

# Farside detector at (30 deg, 160 cm)
/placement/setAngle 30
/placement/setDistance 160

/run/printProgress 10
/run/beamOn 200
